<style>
  table
  {
    width: 100%;
    border: 1px solid;
  }

  td
  {
    text-align: left;
    padding-left: 10px;
  }

  th
  {
    background-color: rgb(179, 179, 179);
  }

</style>



<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Class</th>
    </tr>
  </thead>
  <tbody>
      <% users.forEach(function(user){ %>
        <tr>
          <td><%=user.name%></td>
          <td><%=user.class%></td>
        </tr>
      <% }); %>
  </tbody>
</table>